#!/bin/bash

killall dunst

nohup dunst >/dev/null 2>&1 &

notify-send "CIAO" "come va? bene e tu?" -i mail-attachment-symbolic -u low
notify-send "CIAO 2" "come va? bene e tu? asdoajojoa asodjoia asodno" -i mail-attachment-symbolic
notify-send "CIAO NORMAL" "bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaooi"
notify-send "CIAO NORMAL" "bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaooi"
dunstify --appname="Ciao Test" --action="replyAction,reply" "Message received" "bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaooi" --icon=mail-message-new -u critical
notify-send "EHILA LOW" "bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaooi" -u low
notify-send "CIAO NORMAL 2" "bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaoou bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaoou bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaoou  bla bla sadno asodi, oasdo oasdoj oaaksd, asdjojaooi"
